{
  "manifest_version": 2,

  "name": "Google Chrome Nail Extension",
  "description": "Add nail extensions to every webpage",
  "version": "0.1",

  "icons": {
    "48": "icon.png"
  },

  "permissions": ["tabs", "<all_urls>"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
